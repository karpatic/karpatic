{
  "name": "karpatic",
  "description": "DESCRIPTION",
  "author": "Charles Karpati",
  "license": "MIT",
  "scripts": {
    "start": "webpack serve --mode development --devtool eval-source-map --progress",
    "bild": "webpack --mode production --profile --progress",
    "deploy": "make pages; npm run bild",
    "postbild": "react-snap",
    "postreact-snap": "ls",
    "server": "nodemon --optimize_for_size --max_old_space_size=460 src/server/index.js",
    "test": "nodemon src/server/scripts/test.js",
    "watchPages": "nodemon --watch 'src/ipynb' --ext 'ipynb' --exec 'npm run makePages'",
    "makePages": "make pages",
    "makeAudio": "make audio",
    "pydia": "node ./src/tests/diagram.js" 
    
  },
  "reactSnap": {
    "headless": true,
    "skipThirdPartyRequests": false,
    "concurrency": 6,
    "inlineCss": false,
    "source": "./docs",
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ],
    "removeStyleTags": false,
    "preloadImages": false,
    "asyncScriptTags": false,
    "removeScriptTags": false,
    "include": [
      "/index.html"
    ],
    "puppeteer": {
      "cache": false
    },
    "minifyHtml": {
      "collapseWhitespace": true,
      "removeComments": true
    },
    "destination": "./docs"
  },
  "browserslist": [
    "defaults"
  ],
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.18.6",
    "axios": "^1.4.0",
    "babel-loader": "^8.2.2",
    "chromium": "^3.0.3",
    "compression-webpack-plugin": "^10.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^5.0.1",
    "cssnano": "^5.1.15",
    "dotenv": "^16.3.1",
    "ethers": "5.7.2",
    "express": "^4.15.2",
    "express-rate-limit": "^6.6.0",
    "fs": "^0.0.1-security",
    "handlebars": "^4.7.7",
    "html-inline-css-webpack-plugin": "^1.11.1",
    "html-minimizer-webpack-plugin": "^4.3.0",
    "html-webpack-inline-source-plugin-patched": "^1.1.1",
    "html-webpack-plugin": "^5.5.0",
    "http-server": "^14.1.1",
    "image-minimizer-webpack-plugin": "^3.8.1",
    "imagemin": "^8.0.1",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^9.0.2",
    "imagemin-svgo": "^10.0.1",
    "isomorphic-fetch": "^3.0.0",
    "marked": "^4.2.12",
    "mini-css-extract-plugin": "^1.3.3",
    "path": "^0.12.7",
    "postcss": "^8.2.1",
    "postcss-loader": "^4.1.0",
    "postcss-preset-env": "^6.7.0",
    "puppeteer": "^19.1.0",
    "react-helmet-async": "^1.3.0",
    "react-snap": "^1.23.0",
    "sass": "^1.30.0",
    "sass-loader": "^10.1.0",
    "sharp": "^0.31.3",
    "webpack": "^5.89.0",
    "webpack-bundle-analyzer": "^4.8.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.11.1",
    "webpack-pwa-manifest": "^4.3.0"
  },
  "dependencies": {
    "acorn": "^8.11.3",
    "nodemon": "^3.0.1",
    "openai": "^4.16.1",
    "web3": "^4.0.3"
  }
}
